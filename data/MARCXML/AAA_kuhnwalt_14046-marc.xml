      <marc21:subfield code="a">Walt Kuhn, Paris, France letter to Vera Kuhn, Chevy Chase, Md.</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
      <marc21:subfield code="c">1912 Oct 28</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="300">
