   <marc21:datafield ind1="0" ind2="0" tag="245">
      <marc21:subfield code="a">Joseph Cornell, New York, N.Y. letter to Marcel Duchamp, Flushing, N.Y.</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
