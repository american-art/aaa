      <marc21:subfield code="a">Michael Lucero letter to Patti Warashina</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
      <marc21:subfield code="c">ca. 1983</marc21:subfield>
