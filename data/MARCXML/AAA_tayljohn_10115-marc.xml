   <marc21:datafield ind1="0" ind2="0" tag="245">
      <marc21:subfield code="a">Isamu Noguchi, Paris, France letter to Andrée Ruellan, New York, N.Y.</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
