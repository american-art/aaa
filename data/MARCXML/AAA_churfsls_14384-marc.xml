   <marc21:datafield ind1="0" ind2="0" tag="245">
      <marc21:subfield code="a">Frederic Stuart Church letter to unidentified recipient with enclosed photo</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
