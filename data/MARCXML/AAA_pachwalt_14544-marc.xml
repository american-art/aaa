      <marc21:subfield code="a">Letter from Maximilian P.E. Groszmann to Gotthelf Pach</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
      <marc21:subfield code="c">1895 Jan. 15</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="300">
