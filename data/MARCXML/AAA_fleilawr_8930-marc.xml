   <marc21:datafield ind1="0" ind2="0" tag="245">
      <marc21:subfield code="a">Charles Ephraim Burchfield letter to Lawrence Arthur Fleischman, Detroit, Mich.</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
      <marc21:subfield code="c">1956 Mar. 17</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="300">
      <marc21:subfield code="a">Letter : 5 p. :</marc21:subfield>
      <marc21:subfield code="b">handwritten ;</marc21:subfield>
      <marc21:subfield code="c">22 x 15 cm.</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="520">
      <marc21:subfield code="a">Burchfield thanks Fleischman for his letter and says he is happy to know that Fleischman has purchased two of his paintings; &lt;em&gt;In a deserted house&lt;/em&gt; and &lt;em&gt;Nighthawks at twilight&lt;/em&gt;. He also tells Fleischman of a cross-country trip he recently made. As a post script he includes a two-page account of the background behind his work &lt;em&gt;The parade&lt;/em&gt;.</marc21:subfield>
   </marc21:datafield>
