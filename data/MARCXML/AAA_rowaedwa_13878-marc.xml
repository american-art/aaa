   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="260">
      <marc21:subfield code="c">1934 Feb. 6</marc21:subfield>
   </marc21:datafield>
   <marc21:datafield ind1=" " ind2=" " tag="300">
